{
    "name": "ac/sendinblue-on-register",
    "type": "wordpress-plugin",
    "require": {
        "sendinblue/api-v3-sdk": "^7.4"
    },
    "require-dev": {
        "humbug/php-scoper": "^0.18"
    },
    "autoload": {
        "psr-4": {
            "ACSB\\SendinblueOnRegister\\": "src/"
        }
    },
    "scripts": {
        "build": [
            "php vendor/humbug/php-scoper/bin/php-scoper add-prefix --config=scoper.inc.php --force --output-dir=build",
            "composer dump-autoload -o -d build"
        ]
    }
}
