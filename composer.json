{
    "name": "ac/sendinblue-on-register",
    "type": "wordpress-plugin",
    "require": {
        "sendinblue/api-v3-sdk": "^7.0"
    },
    "require-dev": {
        "coenjacobs/mozart": "^0.7"
    },
    "extra": {
        "mozart": {
            "dep_namespace": "ACSB\\Vendor\\",
            "dep_directory": "./scoped-vendor/",
            "classmap_directory": "./scoped-classmap/",
            "packages": [
                "sendinblue/api-v3-sdk",
                "guzzlehttp/guzzle",
                "guzzlehttp/promises",
                "guzzlehttp/psr7",
                "psr/http-client",
                "psr/http-message"
            ],
            "override_autoload": {
                "sendinblue/api-v3-sdk": {
                    "SendinBlue\\Client\\": "lib/"
                }
            }
        }
    },
    "autoload": {
        "psr-4": {
            "ACSB\\SendinblueOnRegister\\": "src/"
        },
        "classmap": [
            "scoped-classmap/"
        ]
    },
    "scripts": {
        "mozart": "mozart compose"
    }
}
